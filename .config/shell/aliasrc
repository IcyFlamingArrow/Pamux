alias n='nnn'
alias pamux='dotbare'
alias tsmr='transmission-remote'
alias update='sudo pacman -Syu'
alias updatedb='doas updatedb'
alias v='nvim'
alias va='nvim $HOME/.config/shell/aliasrc'
alias vc='nvim $HOME/.config/cmus/rc'
alias vm='nvim $HOME/.config/mimeapps.list'
alias vv='nvim $HOME/.config/nvim/init.vim'
alias vp='nvim $HOME/.config/shell/profile'

